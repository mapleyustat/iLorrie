<?xml version="1.0" encoding="gb2312"?>
<!-- 配置文件 Config.xml-->
<ConfigRoot>
	<!-- 记录数据结构相关参数的设置文件-->
	<RecordSetting>
		<!-- 设置事件信息-->
		<Event>
			<!-- 设置事件类型-->
			<Type>生活</Type>
			<Type>工作</Type>
			<Type>学习</Type>
			<Type>娱乐</Type>
			<Type>其他</Type>
		</Event>
		<!-- 设置反馈的态度类型-->
		<Feedback>
			<!-- 设置对待事件的态度-->
			<Attitude>很开心</Attitude>
			<Attitude>高兴</Attitude>
			<Attitude>平静</Attitude>
			<Attitude>不愉快</Attitude>
			<Attitude>生气</Attitude>
			<!-- 设置建议类型-->
			<Type>不用这么麻烦:)</Type>
			<Type>很好，保持</Type>
			<Type>还可以</Type>
			<Type>有待改进</Type>
			<Type>不许再犯</Type>
		</Feedback>
		<Effect>
			<!-- 设置事件类型-->
			<Level>效果很好</Level>
			<Level>改进后很有效果</Level>
			<Level>不需要改进</Level>
			<Level>改进后无甚效果</Level>
			<Level>不知如何改进</Level>
		</Effect>
	</RecordSetting>
</ConfigRoot>
